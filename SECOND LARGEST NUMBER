#include<stdio.h>
void main()
{
    int a,b,c;
    printf("Enter any three numbers: ");
    scanf("%d%d%d",&a,&b,&c);
    if(a>b && a>c)
    {
        if (b>c)
         printf("second largest number is %d:",b);
        else
         printf("second largest number is %d:",c);
         
    }
    else if(b>a && b>c)
    {
        if(a>c)
         printf("second largest number is %d:",a);
        else
         printf("second largest number is %d:",c);
    }
    else
    {
        if(b>a)
         printf("second largest number is %d:",b);
        else
         printf("second largest number is %d:",a);
    }
}
